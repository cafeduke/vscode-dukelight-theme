{
	"name": "Duke Light",
	"type": "light",
	"colors": {
		"editor.background": "#f5f5f5",
		"editor.foreground": "#000000",
		"editor.lineHighlightBorder": "#e8e8e8",

		"sideBar.background": "#003c64",
		"sideBarSectionHeader.foreground":"#ffffff",
		"sideBarSectionHeader.background": "#00283c",
		"sideBar.foreground":"#DCDEDF",

		"tab.border": "#252525",
		"tab.activeBackground": "#003c64",
		"tab.activeForeground": "#f5f5f5",
		"tab.inactiveBackground": "#3c3c3c",
		"tab.inactiveForeground": "#c0c0c0",
		"tab.activeBorderTop": "#fd971f",

		"editorGroupHeader.tabsBackground" : "#252525",
		"editorGroupHeader.tabsBorder": "#c0c0c0",

		"list.focusBackground": "#6b9801",
		"list.hoverBackground": "#6b9801",
		"list.activeSelectionBackground": "#0078c8",
		"list.inactiveSelectionBackground": "#00538a",
		"list.errorForeground": "#e65050",
		"activityBar.background": "#00283c",

		"gitDecoration.addedResourceForeground": "#ffffff",
		"gitDecoration.modifiedResourceForeground": "#ffffff",
		"gitDecoration.deletedResourceForeground": "#ffffff",
		"gitDecoration.untrackedResourceForeground": "#ffffff",
		"gitDecoration.ignoredResourceForeground": "#ffffff",
		"gitDecoration.conflictingResourceForeground": "#ffffff",
		"gitDecoration.submoduleResourceForeground": "#ffffff",
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#3f7f5f"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#3f7f5f"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#3f7f5f"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#a05000"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#780050"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#780050"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#780050"
			}
		},

		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#782800"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#785000"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#004080"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#780050"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#780050"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#780050"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#780050"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#780050"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#0078a0"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name":"Duke, storage types. Java,TypeScript",
			"scope": [
				"storage.type.java",
				"storage.type.generic.java",
				"entity.name.type.ts"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0078a0"
			}
		},
		{
			"name": "Duke, Variables",
			"scope": [
				"support.variable"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
  		{
			"name": "Duke, Builtin functions",
			"scope": [
				"support.function.builtin"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#006450"
			}
		},
  	{
			"name": "Duke, instance function name",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"foreground": "#0028a0"
			}
		},
		{
			"name":"Duke, all strings",
			"scope": [
				"string",
				"string.tag",
				"string.value",
				"string.regexp",
				"string.quoted.double.shell",
				"entity.name.operator.custom-literal.string",
				"meta.embedded.assembly",
				"punctuation.definition.string",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Duke, HTML,XML: Tags",
			"scope": [
				"punctuation.definition.tag",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#004080"
			}
		},
  	{
			"name": "Duke, HTML: Doctype Declaration",
			"scope": [
				"meta.tag",
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Duke, HTML tag Name, HTML Tag in JS, XML Tag",
			"scope": [
				"entity.name.tag",
				"support.class.component.js",
				"meta.tag.preprocessor.xml",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#780050"
			}
		},
		{
			"name":"Duke, python doc",
			"scope": [
				"comment",
				"string.quoted.docstring.multi.python",
				"punctuation.definition.string.begin.python",
				"punctuation.definition.string.end.python"
			],
			"settings": {
				"foreground": "#3f7f5f"
			}
		},
		{
			"name": "Duke, coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name":"Duke, annotation",
			"scope": [
				"punctuation.definition.annotation",
				"storage.type.annotation"
			],
			"settings": {
				"foreground": "#782800",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Duke, attribute name in CSS like config",
			"scope": [
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded",
				"support.type.property-name.css",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#004080"
			}
		},
		{
			"name": "Duke, unit like px in CSS",
			"scope": "keyword.other.unit",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"scope": "Duke, punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#782800"
			}
		},
		{
			"scope": "Duke, punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name":"Duke, typescript binding",
			"scope": [
				"entity.other.ng-binding-name"
			],
			"settings": {
				"foreground": "#0050a0"
			}
		},
		{
			"name":"Duke, typescript expression",
			"scope": [
				"expression.ng"
			],
			"settings": {
				"foreground": "#0050a0"
			}
		},
	]
}